{
    "name":"YT2Y",
    "description": "sdrtba",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "js/background.js"
    },
    "permissions": ["activeTab", "scripting", "tabs", "storage"],
    "action": {
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": ["https://*.youtube.com/*", "https://music.yandex.ru/handlers/*"],
        "js": ["js/content.js"]
      }
    ],
    "icons": {
      "16": "ico/logo.png",
      "32": "ico/logo.png",
      "48": "ico/logo.png",
      "128":"ico/logo.png"
    }
}
